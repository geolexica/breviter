{
  "name": "reverse",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "init-dev": "next dev",
    "init-build": "next build",
    "init-start": "next start",
    "dev": "yarn compute_db && yarn init-dev",
    "build": "yarn compute_db && yarn init-build",
    "start": "yarn compute_db && yarn init-start",
    "compute_db": "ts-node --project tsconfig.bin.json bin/compute_db.ts",
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run lint"
  },
  "dependencies": {
    "@blueprintjs/core": "^3.54.0",
    "@blueprintjs/popover2": "^0.14.0",
    "@tensorflow-models/universal-sentence-encoder": "^1.3.3",
    "@tensorflow/tfjs": "^3.20.0",
    "@tensorflow/tfjs-node": "^3.20.0",
    "js-yaml": "^4.1.0",
    "next": "12.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^12.3.1",
    "@types/node": "^14.18.30",
    "@types/react": "17.0.50",
    "gts": "^3.1.1",
    "typescript": "4.8.3"
  }
}
